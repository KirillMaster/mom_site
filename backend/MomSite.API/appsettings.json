{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AdminPassword": "admin",
  "ConnectionStrings": {
    "DefaultConnection": "Host=postgres;Database=mom_site_db;Username=postgres;Password=password"
  },
  "JWT": {
    "Secret": "test-secret-key",
    "Issuer": "mom-site",
    "Audience": "mom-site"
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://0.0.0.0:5000"
      }
    },
    "Limits": {
      "MaxRequestBodySize": 104857600,
      "MaxConcurrentConnections": 100,
      "MaxConcurrentUpgradedConnections": 100
    }
  }
} 